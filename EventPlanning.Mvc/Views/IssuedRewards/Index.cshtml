@model IEnumerable<EventPlanning.Mvc.Models.Entities.IssuedReward>

@{
    ViewBag.Title = "Выданные награды";
}

<h2>Выданные награды</h2>

<p>
    @Html.ActionLink("Создание", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Employee.AspNetUsersId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EventReward.EventRewardId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RemunerationDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AmountRewardIssued)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Employee.AspNetUsersId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EventReward.EventRewardId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RemunerationDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AmountRewardIssued)
        </td>
        <td>
            @Html.ActionLink("Редактирование", "Edit", new { id=item.IssuedRewardId }) |
            @Html.ActionLink("Подробнее", "Details", new { id=item.IssuedRewardId }) |
            @Html.ActionLink("Удаление", "Delete", new { id=item.IssuedRewardId })
        </td>
    </tr>
}

</table>
